(module (func $store_value (param $address i32) (param $value f32) (f32.store (get_local $address) (get_local $value))) (func $load_value (param $address i32) (result f32) (f32.load (get_local $address))) (memory $mem 1) (data (i32.const 2) "\00\00\80\3F") (func $mem_func (local $mem_size i32) (local $mem_grow i32) (local.set $mem_size (memory.size)) (local.set $mem_grow (memory.grow (i32.const 1))) (memory.fill (i32.const 1024) (i32.const 42) (i32.const 100)) (memory.copy (i32.const 1124) (i32.const 1024) (i32.const 100)) (memory.init 0 (i32.const 1224) (i32.const 0) (i32.const 13))))